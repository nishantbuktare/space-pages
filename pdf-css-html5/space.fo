                            <fo:block id="d32e1092"/>
                            <fo:block-container font-family="Montserrat, sans-serif, Segoe UI, Microsoft YaHei, Yu Gothic, Malgun Gothic, Latha, Arial Unicode MS" font-size="2.4em" space-after="1em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title10"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 6 - <!--content(text)--><fo:inline>Space Ships</fo:inline></fo:marker><fo:inline>Chapter 6. </fo:inline><fo:inline>Space Ships</fo:inline></fo:block></fo:block-container>
                            
                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container font-family="Lato, sans-serif, Segoe UI, Microsoft YaHei, Yu Gothic, Malgun Gothic, Latha, Arial Unicode MS" space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Specifications and details
                                    about the X22000 and Z11000 space crafts</fo:block></fo:block-container>
                                <fo:block-container font-family="Lato, sans-serif, Segoe UI, Microsoft YaHei, Yu Gothic, Malgun Gothic, Latha, Arial Unicode MS" space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">A space ship is a vehicle or
                                    machine designed to fly in outer space. Space ships are used for
                                    a variety of purposes, including communications, Earth
                                    observation, meteorology, navigation, space colonization,
                                    planetary exploration, and transportation of humans and cargo.
                                    All space ships, except single-stage-to-orbit vehicles, cannot
                                    get into space on their own, and require a launch vehicle
                                    (carrier rocket).</fo:block></fo:block-container>
                            </fo:block></fo:block-container>
                            <fo:block>
                                <fo:block>
                                    <fo:block-container space-after="0em" space-before="0em" xml:lang="en"><fo:block end-indent="0" start-indent="0">
                                        <fo:block font-weight="bold" page-break-after="avoid">Related
                                            information</fo:block>
                                        <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="40px" space-after="0em" space-before="0" padding-left="0" margin-left="0">
                                            <fo:list-item space-after="0em" space-before="0em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"/></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline color="blue" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" fox:alt-text="Information about NASA's aeronautics research" internal-destination="unique_6">Aeronautics</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" fox:alt-text="Information about NASA's aeronautics research" internal-destination="unique_6"><fo:inline font-style="italic" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6"><fo:page-number-citation ref-id="unique_6"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline></fo:block></fo:list-item-body></fo:list-item>
