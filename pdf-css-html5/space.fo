                                    <fo:block id="d32e1187"/>
                                    <fo:block-container font-family="Montserrat, sans-serif, Segoe UI, Microsoft YaHei, Yu Gothic, Malgun Gothic, Latha, Arial Unicode MS" font-size="1.8em" font-weight="300" space-after="0.4em" margin-left="0" margin-right="0" space-before="1.3em" page-break-after="avoid" id="ariaid-title12"><fo:block end-indent="0" start-indent="0" role="H3"><fo:inline>X22000 Specs</fo:inline></fo:block></fo:block-container>
                                    
